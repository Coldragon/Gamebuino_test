Image spritesheet = Image(Character_sprite);

int player_pos_x = 20, player_pos_y = 20;
int player_speed = 2;
float player_frame_number = 0;
float animation_speed = 0.3f;

void menu_test()
{
	const char* menu[] =
	{
		"Set Player Speed",
		"Set Pos X",
		"Set Pos Y",
		"Set Anim Speed",
	};

	char name[16];

	int value_returned;
	if (gb.buttons.pressed(BUTTON_MENU) && gb.buttons.repeat(BUTTON_B, 0))
	{
		uint8_t entry = gb.gui.menu("DEBUG MENU", menu);
		
		switch (entry)
		{
		case 0:
			value_returned = counter_select("Player Speed", player_speed, 0, 10, YELLOW);
			player_speed = value_returned;
			popup(String("Speed: ") + value_returned);
			break;
		case 1:
			value_returned = counter_select("Player Pos X", player_pos_x, -200, 200, BLUE);
			player_pos_x = value_returned;
			popup(String("PosX: ") + value_returned);
			break;
		case 2:
			value_returned = counter_select("Player Pos Y", player_pos_y, -200, 200, BLUE);
			player_pos_y = value_returned;
			popup(String("PosY: ") + value_returned);
			break;
		case 3:
			value_returned = counter_select("Animation Speed /100", animation_speed*100, 0, 200, PURPLE);
			animation_speed = static_cast<float>(value_returned) / 100.0f;
			popup(String("AnimSpeed: ") + animation_speed);
			break;
		}
	}
}

void move_and_animate(Button button, int x_offset, int y_offset, int frame_start, int frame_end)
{
	if (gb.buttons.repeat(button, 0))
	{
		player_pos_x += x_offset;
		player_pos_y += y_offset;
		if (player_frame_number >= frame_start && player_frame_number <= frame_end)
		{
			player_frame_number += animation_speed;
			if (player_frame_number > frame_end)
				player_frame_number -= (frame_end - frame_start);
		}
		else
			player_frame_number = frame_start;
	}
}

void control()
{
	constexpr auto frame_left_start_animation = 9;
	constexpr auto frame_left_end_animation = 11;
	move_and_animate(BUTTON_LEFT, -1, 0, frame_left_start_animation, frame_left_end_animation);

	constexpr auto frame_right_start_animation = 3;
	constexpr auto frame_right_end_animation = 5;
	move_and_animate(BUTTON_RIGHT, +1, 0, frame_right_start_animation, frame_right_end_animation);

	constexpr auto frame_up_start_animation = 0;
	constexpr auto frame_up_end_animation = 2;
	move_and_animate(BUTTON_UP, 0, -1, frame_up_start_animation, frame_up_end_animation);

	constexpr auto frame_down_start_animation = 6;
	constexpr auto frame_down_end_animation = 8;
	move_and_animate(BUTTON_DOWN, 0, +1, frame_down_start_animation, frame_down_end_animation);
}

void setup()
{
	gb.begin();
	DEBUG_INIT();
}

void loop()
{
	while (!gb.update());
	menu_test();
	control();
	// Background
	gb.display.clear();
	gb.display.setColor(BLACK);
	gb.display.fill();
	// Draw Sprite
	spritesheet.setFrame(player_frame_number);
	gb.display.drawImage(player_pos_x, player_pos_y, spritesheet);
}